<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/coral.css")
    @Scripts.Render("~/coral.js")

</head>
<body>
    <div id="content">
        <div class="navbar navbar-primary main" style="visibility: visible;">
            <div class="col-md-3">
                <div class="input-group">
                    <p class="logo">Intellectus Training</p>
                </div>
            </div>
            <div class="col-md-5 padding-none visible-md visible-lg">
                <div class="input-group innerLR">
                    <input type="text" class="form-control" id="input-sm" placeholder="a proucura de um presente ou de alguem para te entregar?">
                    <span class="input-group-btn">
                        <button class="btn btn-primary" type="button"><i class="glyphicon glyphicon-search font20"></i></button>
                    </span>
                </div><!-- /input-group -->
            </div>
            <div class="col-md-4">
                <div class="input-group pull-right">
                   
                    <span class="space-20-r"></span>
                    
                </div>
            </div>
        </div>
        <div class="container">
            @RenderBody()
            <footer>
                <p>&copy; @DateTime.Now.Year - Jefferson Rossi Florencio (Jefferson.florencio@live.com)</p>
            </footer>
        </div>
           
       
    </div>
    

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)
</body>
</html>
